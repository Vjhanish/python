class Solution(object):
    def countPalindromicSubsequence(self, s):
        """
        :type s: str
        :rtype: int
        """
        letters = set(s)
        ans = 0
        for letter in letters:
            start,end = s.index(letter),s.rindex(letter)
            mid_letters = len(set(s[start+1:end]))
            ans += mid_letters
        return ans
