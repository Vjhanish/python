#KN
class Solution(object):
    def rangeAddQueries(self, n, queries):
        
        mat = [[0]*n for _ in range(n)]
        
        for r in range(n):
            diff = [0] * (n + 1)  # Difference array for current row
            for r1, c1, r2, c2 in queries:
                if r1 <= r <= r2:  
                    diff[c1] += 1      # Start of range
                    diff[c2 + 1] -= 1  # End of range
            
            # Build final row using cumulative sum
            CUM_sum = 0
            for c in range(n):
                CUM_sum += diff[c]
                mat[r][c] = CUM_sum
        
        return mat
