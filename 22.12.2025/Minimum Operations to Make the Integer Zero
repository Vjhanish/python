class Solution:
    def makeTheIntegerZero(self, num1, num2):
        for k in range(1, 61):  # check up to 60 operations
            target = num1 - k * num2
            if target < k:  # target must be >= k
                continue
            if bin(target).count("1") <= k:  # check if representable with k powers of 2
                return k
        return -1
